	width: 100%;
	height:100%;
}


.active .btm-box{
	bottom: 40px;
}

.btm-box{
	position: absolute;
	bottom: 66px;
	left: 5%;
	background: rgba(0,0,0,0.5);
	width: 97%;
	height: 180rpx;
	box-sizing: border-box;
	padding: 5%;
	
}
.two-color{
	padding:20rpx 0rpx 14rpx;
