.banner{
}
swiper{
  width: 800rpx;
  height: 650rpx;
}
swiper-item{
	padding-top: 40rpx;
}
swiper image{
	border-radius: 30rpx;
	width: 500rpx;
	height: 524rpx;
	position: absolute;
	margin-left: 3%;
}
swiper image.active{
  transform: scale(1.14);
  transition:all .2s ease-in 0s;
  margin-left: 50rpx;
}

.bigbox{
	width: 100%;
