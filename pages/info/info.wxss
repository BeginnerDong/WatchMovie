.box{
	box-sizing: border-box;
	padding: 0rpx 4%;
}
.flo-right image{
	width: 12rpx;
	height: 20rpx;
	margin-left: 50rpx;
	position: relative;
}
.flo-right{
	color: rgb(174,174,174);
	margin-top: 10rpx;
}
.list-info{
	padding: 30rpx 0rpx;
	box-sizing: border-box;
	/* border-bottom: solid 1rpx #E9E9E9; */
	position: relative;
}
.list-left{
	margin-right: 50rpx;
}
.info-info{
	padding: 20rpx 0rpx;
	box-sizing: border-box;
	border-bottom: solid 1rpx #E9E9E9;
	}
.star image{
	width: 28rpx;
	height:28rpx;
	margin-right: 20rpx;
	
}
.list-info input{
	display: inline-block;
	width: 80%;
	height: 100%;
	position: absolute;
	top: 0rpx;
}
.photo{
	width: 30%;
	margin: 0 2%;
	height: 270rpx;
	margin-top: 30rpx;
}
.photo image{
	width: 100%;
	height: 100%;
}
.list-img{
	width: 30rpx;
	height: 24rpx;
	margin-right: 20rpx;
	position: relative;
	top: 2rpx;
}
.bg2{
	width: 100%;
	height: 20rpx;
}
.libe{
	width: 15%;
	height: 50rpx;
	border: solid 1rpx #D2D2D2;
	color: rgb(102,102,102);
	font-size: 26rpx;
	text-align: center;
	line-height: 50rpx;
	margin: 0rpx 4.5% 30rpx;
	display: inline-block;
}

textarea{
	border: solid 1rpx #C5C5C5;
	width: 100%;
	height: 400rpx;
	box-sizing: border-box;
	padding: 30rpx;color: rgb(191,191,191);
	line-height: 34rpx;
} 