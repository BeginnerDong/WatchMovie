@import "../../wxParse/wxParse.wxss";
.top{
	text-align: center;
	width: 100%;
	height: 90rpx;
	line-height: 90rpx;
}
.top-img{
	width: 100%;
	height: 400rpx;
}
.top-img image{
	width: 100%;
	height: 100%;
}

.introduce{
	
	box-sizing: border-box;
	padding:50rpx 30rpx 30rpx;
}
.line-red{
	border-left: solid 6rpx #FF5BA0;
	text-indent: 20rpx;
}
.one-color{
	line-height:36rpx ;
	margin: 0rpx 20rpx;
}
.one-color view{
	width:82rpx;
	height: 36rpx;
	margin-right: 20rpx;
	text-align: center;
}
.textone{
	text-align: justify;
	line-height: 36rpx;
	margin-top: 30rpx;
}
.bg2{
	width: 100%;
	height: 10rpx;
}
.img-one{
	width: 144rpx;
	height: 170rpx;
	display: inline-block;
	overflow: hidden;
	margin-right: 30rpx;
}
.img-one image{
	width: 100%;
	height: 100%;
}

.head{
	width: 84rpx;
	height: 84rpx;
	margin-right: 30rpx;
	overflow: hidden;
}
.head image{
	width: 84rpx;
	height: 84rpx;
}
.img-two{
	width: 30%;
	height: 190rpx;
	overflow: hidden;
	display: inline-block;
	margin-right: 30rpx;
}
.img-two image{
	width: 100%;
	height: 100%;
}
.btm{
	width: 100%;
	height: 80rpx;
	position: fixed;
	bottom: 0rpx;
	background: linear-gradient(to right,#EF2E81,#FF5BA0);
	box-sizing: border-box;
	padding:0rpx 30rpx;
	line-height: 80rpx;
}
page{
	width: 100%;
	overflow-x: hidden
}



