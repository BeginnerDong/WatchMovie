page{
	background: #F4F4F4;
}
.bigbox{
	width: 92%;
	margin: 4%;
	padding: 4%;
	box-shadow: 0rpx 0rpx 14rpx #C4BCBF;
	border-radius: 20rpx;
	box-sizing: border-box;
}
.itemimg{
	width: 70rpx;
	height:70rpx;
}
.itemimg image{
	width: 100%;
	height: 100%;
}

.boxnum-img{
	margin-right: 50rpx;
}
.introduce{
	width: 100%;
	box-sizing: border-box;
	padding:30rpx;
}

.img-box{
	width: 30%;
	height: 206rpx;
	margin-right: 3%;
}
.img-box image{
	width: 100%;
	height: 100%;
}

.head{
	width: 84rpx;
	height: 84rpx;
	margin-right: 30rpx;
	overflow: hidden;
	}
.head image{
	width: 84rpx;
	height: 84rpx;
}
.head-two{
	width:60rpx;
	height:60rpx;
	border-radius: 50%;
	overflow: hidden;
}
.head-two image{
	width:100%;
	height: 100%;
}
.name{
	width: 50%;
	margin-left: 20rpx;
	position: relative;
	top:-16rpx;
}
.boxnum-img{
	margin-left: 42%;
}
.box-text{
	line-height: 36rpx;
	margin-top: 20rpx;
}
.open{
	text-align: center;
}
.open image{
	width: 20rpx;
	height: 12rpx;
	margin-left: 20rpx;
	position: relative;
	top: -4rpx;
}

.btm{
	width: 100%;
	height: 120rpx;
	position: fixed;
	bottom: 0rpx;
	box-sizing: border-box;
	padding: 0rpx 30rpx;
	border-top: solid 1rpx #EEEEEE;
}
.btm input{
	width: 80%;
	height: 60rpx;
	border-radius: 40rpx;
	line-height: 60rpx;
	margin-top: 30rpx;
	background: #EFEFEF;
	box-sizing: border-box;
	padding-left: 30rpx;
}



