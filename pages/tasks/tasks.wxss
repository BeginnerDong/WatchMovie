page{
	background: #F2F2F2;
}
.top{
	width: 100%;
	height: 400rpx;
}
.top-box{
	width: 100%;
	text-align: center;
	padding: 50rpx 39.5% ;
}
.top-img{
	width: 160rpx;
	height:160rpx;
	text-align: center;
	position: relative;
	box-sizing: border-box;
	
}
.top-img image{
	width: 100%;
	height: 100%;
}
.top-img view{
	font-size: 44rpx;
	color: rgb(128,66,12);
	position: absolute;
	top: 50rpx;
	left:34rpx;
}
.box-two{
	box-sizing: border-box;
	padding: 30rpx 0rpx;
	margin-bottom: 10rpx;
}
.box-two image{
	width: 30rpx;
	height: 30rpx;
	margin-left: 20rpx;
}
.line-box{
	box-sizing: border-box;
	padding: 30rpx 4% 0px;
}
.line-red{
	border-left: solid 6rpx #FF5BA0;
	box-sizing: border-box;
	padding-left:30rpx;
}
.list-info{
	box-sizing: border-box;
	padding: 30rpx;
	border-bottom: solid 1rpx #F2F2F2;
}
.list-color{
	width: 70rpx;
	height: 30rpx;
	background: #FFF8D4;
	text-align: center;
	line-height: 30rpx;
	margin-left: 50rpx;
	}
.list-color image{
	width: 16rpx;
	height: 16rpx;
}
.list-info .flo-right{
	width: 110rpx;
	height:40rpx;
	border: solid 1rpx #FF5BA0;
	color: #FF5BA0;
	line-height: 40rpx;
	text-align: center;
}


.secceed{
	position: fixed;
	z-index: 1000;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,0.5);
	top: 0rpx;
}
.secceed .text{
	text-align: center;
	position: absolute;
	top: 69%;
	left: 29%;
}
.secceed-box{
	position: relative;
}
.secceed-box image{
	width: 100%;
	height: 100%;
}


