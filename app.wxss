/***********navbar***********/
.navbar{
	height:110rpx;
	width:100%;
	position:fixed;
	bottom:0rpx;
	z-index:999;
	background:#fff;
	display:flex;
	justify-content:center;
	align-items:center;
	box-shadow:0rpx 0rpx 15rpx #D5D5D5;
}
.nav_img image{
	width:44rpx;
	height:44rpx;
}
.navbar_item{
	width:33.3%;
	text-align:center;
}
.navbar .text{
	color:#999;
	font-size:22rpx;
}
.navbar .this-text{
	color:#FF5BA0;
}
.nav_img{
	width:48rpx;
	height:48rpx;
	margin:0 auto;
}

